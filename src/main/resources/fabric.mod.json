{
  "schemaVersion": 1,
  "id": "themelio",
  "version": "${version}",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.frontear.themelio.ExampleMod"
    ]
  },
  "mixins": [
    "mixins.themelio.json"
  ],

  "depends": {
    "fabricloader": ">=${loader}",
    "minecraft": "${minecraft}"
  },

  "name": "Themelio",
  "description": "Themelio is a basic gradle environment designed for quick setup so that you may easily begin writing a fabric mod",
  "contact": {
    "issues": "https://github.com/Frontear/Themelio/issues",
    "sources": "https://github.com/Frontear/Themelio"
  },
  "authors": [
    "Frontear"
  ],
  "contributors": [],
  "license": "GPL-3.0-only",
  "icon": "assets/themelio/icon.png"
}